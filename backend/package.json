{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "dev:tsx": "tsx watch src/index.ts",
    "dev:debug": "node --inspect --loader tsx/esm src/index.ts",
    "dev:debug-brk": "node --inspect-brk --loader tsx/esm src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts",
    "migrate:status": "knex migrate:status --knexfile knexfile.ts",
    "seed:run": "knex seed:run --knexfile knexfile.ts",
    "seed:make": "knex seed:make --knexfile knexfile.ts",
    "seed:dev": "tsx scripts/seed-dev-data.ts",
    "seed:validate": "tsx scripts/validate-seeds.ts"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.7",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "tesseract.js": "^6.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^7.0.2",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/tesseract.js": "^0.0.2",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@esbuild/win32-x64": "^0.25.10"
  }
}
